{
  "name": "bookshop",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "dependencies": {
    "@sap/cds": "^9",
    "express": "^4"
  },
  "devDependencies": {
    "@cap-js/sqlite": "^2",
    "cds-plugin-ui5": "^0.13.0"
  },
  "scripts": {
    "start": "cds-serve",
    "watch-pauls-bookshop": "cds watch --open paulsbookshop/index.html?sap-ui-xx-viewCache=false --livereload false"
  },
  "private": true,
  "sapux": [
    "app/pauls-bookshop"
  ],
  "cds": {
    "requires": {
      "db-ext": {
        "[development]": {
          "model": "db/sqlite"
        },
        "[production]": {
          "model": "db/hana"
        }
      }
    }
  },
  "workspaces": [
    "app/*"
  ]
}